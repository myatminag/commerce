model Cart {
  id          String   @id @default(cuid())
  productId   String
  product     Product  @relation(fields: [productId], references: [id])
  customerId  String
  customer    Customer @relation(fields: [customerId], references: [id])
  note        String?
  subTotal    Int
  deliveryFee Int
  taxFee      Int
  discount    Int
  totalAmount Int
  createdAt   DateTime @default(now())
  order       Order[]
}
