model Vendor {
  id             String    @id @default(cuid())
  name           String    @db.VarChar(255)
  slug           String    @unique @db.VarChar(255)
  bannerImage    Json
  thumbnailImage Json
  description    String
  created_at     DateTime  @default(now())
  updated_at     DateTime  @updatedAt
  Product        Product[]
}
